#
# sh/ksh initialization

LANG=en_US.UTF-8
EDITOR=vim
PAGER=less
PATH=$HOME/bin:$HOME/bin/vim/bin:/bin:/sbin:/usr/bin:/usr/sbin:/usr/X11R6/bin:/usr/local/bin:/usr/local/sbin:/usr/games
ENV=$HOME/.kshrc
TERM=xterm-256color

FZF_DEFAULT_COMMAND="rg --files --no-ignore-vcs --hidden"
GOPATH=$HOME/code/go

export PATH HOME TERM EDITOR PAGER LANG ENV FZF_DEFAULT_COMMAND GOPATH

